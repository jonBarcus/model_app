<header>
	<h1>The Model App</h1>
</header>
<section>
	<% if user_signed_in? %>
	<%= link_to 'Start a New Club', new_club_path %>
	<%= link_to "All Your Clubs", clubs_path %>

	<% @users_clubs.each do |club| %>
	<li>
		<%= link_to(club.name) %>
	</li>
	<% end %>
	<p>Cool stuff should be here eventually</p>
	<%= link_to "Log Out", destroy_user_session_path, method: :delete %>

	<% else %>
	<%# TODO not sure if this is necessary or will work, aim is to have a single page %>
		<%= render "devise/sessions/new.html.erb" %>
	<% end %>
</section>